{
    "name": "hyperplural/warface-sdk",
    "version": "5.2.0",
    "description": "Fast and flexible SDK client of the Warface API in PHP.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/discovery": "^1.13",
        "ext-json": "*",
        "php-http/message": "^1.0",
        "php-http/client-common": "^2.0",
        "fig/http-message-util": "^1.1"
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.6",
        "php-http/mock-client": "^1.5",
        "phpstan/phpstan": "^1.8.6",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.9",
        "rector/rector": "^0.18"
    },
    "autoload": {
        "psr-4": {
            "Hyperplural\\WarfaceSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyperplural\\WarfaceSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test:unit": "vendor/bin/phpunit",
        "test:types": "vendor/bin/phpstan analyse",
        "test:cs": "vendor/bin/phpcs --standard=phpcs.xml src tests",
        "cs:fix": "vendor/bin/phpcbf --standard=phpcs.xml src tests",
        "rector:dry": "vendor/bin/rector process --dry-run",
        "rector:fix": "vendor/bin/rector process",
        "test:coverage": "vendor/bin/phpunit --coverage-clover build/coverage/clover.xml",
        "coverage:check": "php tools/coverage-check.php build/coverage/clover.xml 1.00",
        "phpstan:baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": [
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable"
}
